package error_library
public

annex EMV2 {**
	error types
		NoPower   				: type;
		ValueError				: type;
		NoValue					: type extends ValueError;
		InvalidValue			: type extends ValueError;
		HardwareFailure			: type;
		SoftwareFailure			: type;
	end types; 
 
	error behavior stateMachine
	states
		Operational : initial state;
		Failed      : state;
	end behavior;
**};

end error_library;